@if( $%s->rows()->isEmpty() )
    <tr>
        <td colspan="{{ count( $%s->columns() ) }}">{!! $%s->noRowsMessage() !!}</td>
    </tr>
@else
    @foreach( $%s->rows() as $row )
        <tr>
            @foreach( $%s->columns() as $column )
                <td>{!! $column->render( $row ) !!}</td>
            @endforeach
        </tr>
    @endforeach
@endif
